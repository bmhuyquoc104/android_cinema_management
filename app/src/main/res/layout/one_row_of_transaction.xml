<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <androidx.cardview.widget.CardView
        style="@style/CardView.Light"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="10dp"
        android:layout_marginTop="15dp"
        android:layout_marginEnd="10dp"
        app:cardBackgroundColor="@color/colorPrimary"
        app:cardCornerRadius="7dp"
        app:cardElevation="10dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.032"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">


        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent">


            <TextView
                android:id="@+id/list_of_transaction_date_tv"
                android:layout_width="249dp"
                android:layout_height="17dp"
                android:layout_marginTop="16dp"
                android:paddingStart="25dp"
                android:paddingEnd="25dp"
                android:text="@string/textview"
                android:textColor="@color/colorPrimary4"
                android:textSize="14sp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.005"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

            <TextView
                android:id="@+id/list_of_transaction_time_tv"
                android:layout_width="136dp"
                android:layout_height="19dp"
                android:layout_marginTop="16dp"
                android:paddingStart="25dp"
                android:paddingEnd="25dp"
                android:text="@string/textview"
                android:textColor="@color/colorPrimary4"
                android:textSize="14sp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="1.0"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" />


            <TextView
                android:id="@+id/list_of_transaction_price_tv"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginTop="112dp"
                android:lineSpacingExtra="8sp"
                android:paddingStart="25dp"
                android:paddingEnd="25dp"
                android:text="@string/textview"
                android:textAlignment="textStart"
                android:textColor="@color/colorPrimary4"
                android:textSize="16sp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.0"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

            <TextView
                android:id="@+id/list_of_transaction_seeMore_tv"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginTop="140dp"
                android:layout_marginBottom="10dp"
                android:paddingStart="25dp"
                android:paddingEnd="25dp"
                android:text="@string/seeMore"
                android:stateListAnimator="@android:animator/fade_out"
                android:textAlignment="textEnd"
                android:textColor="@color/sign_in_link_color"
                android:textSize="14sp"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.0"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintVertical_bias="0.0" />

            <TextView
                android:id="@+id/list_of_transaction_screen_tv"
                android:layout_width="195dp"
                android:layout_height="wrap_content"
                android:layout_marginTop="80dp"
                android:lineSpacingExtra="8sp"
                android:paddingStart="25dp"
                android:paddingEnd="25dp"
                android:text="@string/textview"
                android:textColor="@color/colorPrimary4"
                android:textSize="16sp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.005"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

            <TextView
                android:id="@+id/list_of_transaction_seat_tv"
                android:layout_width="195dp"
                android:layout_height="wrap_content"
                android:layout_marginTop="80dp"
                android:lineSpacingExtra="8sp"
                android:paddingStart="25dp"
                android:paddingEnd="25dp"
                android:text="@string/textview"
                android:textColor="@color/colorPrimary4"
                android:textSize="16sp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="1.0"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

            <TextView
                android:id="@+id/list_of_transaction_movie_tv"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginTop="48dp"
                android:letterSpacing="0.10"
                android:paddingStart="25dp"
                android:paddingEnd="25dp"
                android:text="@string/textview"
                android:textAlignment="center"
                android:textColor="@color/colorPrimary2"
                android:textSize="18sp"
                android:textStyle="italic"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.0"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

        </androidx.constraintlayout.widget.ConstraintLayout>

    </androidx.cardview.widget.CardView>


</androidx.constraintlayout.widget.ConstraintLayout>